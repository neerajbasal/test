<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:gravity="bottom"
	android:id="@+id/list1">
	<TextView android:id="@+id/eventDate" android:background="#0000ff"
		android:textColor="#ffffff" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_gravity="left"
		android:textSize="15dp" android:text="@string/name" />
	<TextView android:id="@+id/eventName" android:background="#ffffff"
		android:textColor="#151B8D" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_gravity="left"
		android:textSize="15dp" android:text="@string/name" />

	<TextView android:id="@+id/eventLocation" android:background="#ffffff"
		android:textColor="#151B8D" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_gravity="left"
		android:textSize="15dp" android:text="@string/name" />

</LinearLayout>
